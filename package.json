{
  "name": "eslint-config-rascal-monorepo",
  "version": "0.0.10",
  "private": true,
  "description": "RascalCoder's eslint config presets with prettier",
  "author": "Rascal-Coder <meno.qiqio@gmail.com> (https://github.com/Rascal-Coder)",
  "license": "MIT",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "lint": "eslint . --fix",
    "test": "pnpm -r run test",
    "publish-pkg": "pnpm -r publish --access public",
    "update-version": "bumpp package.json packages/*/package.json --commit --push --tag",
    "release": "pnpm update-version && pnpm publish-pkg",
    "sync": "node ./scripts/sync.mjs",
    "init-husky": "pnpm husky install"
  },
  "devDependencies": {
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "eslint-config-rascal": "workspace:*",
    "husky": "^8.0.1",
    "typescript": "^4.8.4",
    "zx": "^7.1.1"
  }
}
